	"@nestjs/throttler": "^2.0.1",
	"@nestjs/typeorm": "7.1.5",
	"axios": "^0.27.2",
	"cache-manager": "^4.0.0",
	"cache-manager-redis-store": "^2.0.0",
	"class-transformer": "^0.5.1",
