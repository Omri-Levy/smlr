	"csurf": "^1.11.0",
	"ejs": "^3.1.8",
	"helmet": "^5.1.0",
	"mysql2": "^2.3.3",
	"nanoid": "^3.3.4",
	"redis": "3",
	"reflect-metadata": "^0.1.13",
	"rimraf": "^3.0.2",
	"rxjs": "^7.2.0",
	"swagger-ui-express": "^4.4.0",
	"typeorm": "0.2"
  },
  "devDependencies": {
	"@commitlint/cli": "^17.0.1",
